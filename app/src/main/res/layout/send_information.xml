<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:orientation="vertical"
android:background="@color/grey">

<ImageView
    android:id="@+id/keyImage"
    android:layout_width="37dp"
    android:layout_height="44dp"
    android:layout_marginTop="173dp"
    android:layout_marginStart="39dp"


    android:src="@mipmap/ic_launcher"
    tools:layout_editor_absoluteY="114dp" />

<TextView
    android:id="@+id/addressText"
    android:layout_width="282dp"
    android:layout_height="33dp"
    android:layout_marginStart="93dp"
    android:layout_marginTop="180dp"
    android:layout_marginEnd="95dp"
    android:gravity="center_horizontal"
    android:text="Hegdehaugsveien 30"
    android:textColor="@color/black"
    android:textSize="16sp" />
<ImageView
    android:id="@+id/natureImage"
    android:layout_width="37dp"
    android:layout_height="44dp"
    android:layout_marginTop="247dp"
    android:layout_marginStart="39dp"
    android:src="@mipmap/ic_launcher"
    tools:layout_editor_absoluteY="114dp" />
<TextView
    android:id="@+id/natureText"
    android:layout_width="282dp"
    android:layout_height="33dp"
    android:layout_marginStart="93dp"
    android:layout_marginTop="247dp"
    android:layout_marginEnd="95dp"
    android:gravity="center_horizontal"
    android:text="Natur og gront"
    android:textColor="@color/black"
    android:textSize="16sp" />
<ImageView
    android:id="@+id/cameraImage"
    android:layout_width="37dp"
    android:layout_height="44dp"
    android:layout_marginTop="330dp"
    android:layout_marginStart="39dp"
    android:src="@drawable/camera_image"
    tools:layout_editor_absoluteY="114dp" />
<ImageView
    android:id="@+id/voiceRecord"
    android:layout_width="37dp"
    android:layout_height="44dp"
    android:layout_marginTop="414dp"
    android:layout_marginStart="39dp"
    android:src="@drawable/voice_black_24dp"
    tools:layout_editor_absoluteY="114dp" />
<EditText
    android:id="@+id/messageText"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="93dp"
    android:layout_marginTop="420dp"
    android:layout_marginEnd="26dp"
    android:gravity="center_horizontal"
    android:hint="Legg gjerne inn en beskrivelse av ideen Maks 300 tegn"
    android:textColorHint="@color/hintText"
    android:maxLength="300"
    android:textColor="@color/black"
    android:textSize="16sp" />
<ImageView
    android:id="@+id/personImage"
    android:layout_width="37dp"
    android:layout_height="44dp"
    android:layout_marginTop="490dp"
    android:layout_marginStart="39dp"
    android:src="@mipmap/ic_launcher"
    tools:layout_editor_absoluteY="114dp" />
<TextView
    android:id="@+id/mailId"
    android:layout_width="282dp"
    android:layout_height="33dp"
    android:layout_marginStart="93dp"
    android:layout_marginTop="507dp"
    android:layout_marginEnd="95dp"
    android:gravity="center_horizontal"
    android:text="prototype@test.no"
    android:textColor="@color/black"
    android:textSize="16sp" />
<Button
    android:id="@+id/sendInn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="600dp"
    android:layout_marginStart="58dp"
    android:layout_marginEnd="58dp"
    android:alpha="0.5"
    android:background="@drawable/login_button_background"
    android:textColor="@color/white"
    android:text="Send inn" />

</RelativeLayout>